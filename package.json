{
  "name": "proheader",
  "displayName": "proHeader",
  "description": "Header pro for C/H CPP/HPP files",
  "version": "2.0.0",
  "icon": "images/icon.png",
  "license": "MIT",
  "publisher": "Shirosaaki",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shirosaaki/custom_header.git"
  },
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:proheader.insertHeader",
    "onCommand:proheader.helloWorld"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "proheader.insertHeader",
        "title": "Insert Pro Header"
      },
      {
        "command": "proheader.helloWorld",
        "title": "Hello World"
      }
    ],
    "keybindings": [
      {
        "command": "proheader.insertHeader",
        "key": "ctrl+shift+h",
        "when": "editorTextFocus"
      }
    ]
    ,
    "configuration": {
      "title": "Pro Header",
      "properties": {
        "proheader.defaultAuthor": {
          "type": "string",
          "default": "",
          "description": "Default author name used to prefill the Author input. Saved when you enter an author."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.32.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
